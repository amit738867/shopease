{
  "expo": {
    "name": "shopease",
    "slug": "shopease",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/one.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/one.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.shopease.base",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/one.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/one.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "945d3a1e-9bff-43d6-804e-751957518378"
      }
    }
  }
}
